{
  "name": "webtreemap-cdt",
  "version": "3.0.0",
  "description": "treemap visualization",
  "main": "build/src/index.js",
  "bin": {
    "webtreemap": "build/src/cli.js"
  },
  "dependencies": {
    "commander": "^2.11.0"
  },
  "devDependencies": {
    "@types/commander": "^2.11.0",
    "@types/node": "^8.0.34",
    "prettier": "1.14.3",
    "rollup": "^1.19.4",
    "rollup-plugin-banner": "^0.2.1",
    "typescript": "^3.1.3"
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "fmt": "prettier --write src/*.ts demo/*.ts *.md",
    "build": "tsc && rollup -c",
    "demo": "gdu -ab node_modules/ | node build/cli.js --title 'node_modules for webtreemap' > docs/index.html",
    "roll": "yarn build; gcp -f dist/webtreemap.js ~/chromium/src/third_party/blink/renderer/devtools/front_end/webtreemap/webtreemap.js"
  },
  "files": [
    "build/*.js",
    "build/*.d.ts",
    "dist/webtreemap.js"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/paulirish/webtreemap-cdt.git"
  },
  "keywords": [
    "treemap",
    "visualization"
  ],
  "author": "Evan Martin <evan.martin@gmail.com>",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/paulirish/webtreemap-cdt/issues"
  },
  "homepage": "https://github.com/paulirish/webtreemap-cdt",
  "prettier": {
    "singleQuote": true,
    "trailingComma": "es5",
    "bracketSpacing": false
  }
}
